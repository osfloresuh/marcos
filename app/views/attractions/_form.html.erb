<%= simple_form_for @attraction, url:  attractions_path do |f| %>
  <%= select_tag(:category_id, options_for_select(@categories), :prompt => "select a
  category")%>

  <%= f.input :item_img, as: :file %>
  <%= f.input :name, label: "Name" %>
  <%= f.input :description %>
  <%= f.submit "Create Item", :class => 'btn-custom2' %>

<% end %>

